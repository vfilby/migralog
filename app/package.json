    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.3",
    "jest": "^30.2.0",
