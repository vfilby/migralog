{
  "name": "migralog",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "npm run generate-build-info && expo start --dev-client",
    "android": "npm run generate-build-info && expo start --android",
    "ios": "npm run generate-build-info && expo start --ios",
    "web": "npm run generate-build-info && expo start --web",
    "generate-build-info": "node scripts/generate-build-info.js",
    "build:ios": "eas build --platform ios --non-interactive --auto-submit",
    "submit:ios": "eas submit -p ios",
    "precommit": "npm run test:lint:ci && npx tsc --noEmit && npm run test:ci",
    "test:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test:lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test:lint:ci": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:ui": "npx detox test --configuration ios.sim.debug --maxWorkers 3",
    "test:ui:build": "npm run generate-build-info && npx detox build -c ios.sim.debug",
    "sim:ios:load-db": "bash scripts/load-test-db.sh",
    "sim:ios:app": "bash scripts/launch-simulator.sh",
    "eas-build-post-install": "node scripts/generate-build-info.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@sentry/react-native": "~7.2.0",
    "@types/jest": "29.5.14",
    "date-fns": "^4.1.0",
    "eas": "^0.1.0",
    "expo": "^54.0.21",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.16",
    "expo-image-picker": "^17.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-sharing": "~14.0.7",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "jest": "29.7.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-get-random-values": "^2.0.0",
    "react-native-maps": "^1.26.18",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "^0.21.2",
    "sqlite3": "^5.1.7",
    "ulidx": "^2.4.1",
    "whatwg-url-without-unicode": "^8.0.0-3",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.3",
    "better-sqlite3": "^12.4.1",
    "detox": "^20.43.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "jest-circus": "^30.2.0",
    "react-test-renderer": "^19.1.0",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-svg"
      ]
    }
  },
  "private": true
}
