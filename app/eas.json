{
  "cli": {
    "version": ">= 14.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "prebuildCommand": "npm run generate-build-info",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "store",
      "autoIncrement": true,
      "prebuildCommand": "npm run generate-build-info",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "prebuildCommand": "npm run generate-build-info",
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "ascAppId": "6753635113"
      }
    },
    "production": {
      "ios": {
        "ascAppId": "6753635113"
      }
    }
  }
}
