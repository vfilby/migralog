{
  "cli": {
    "version": ">= 14.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "SENTRY_DISABLE_AUTO_UPLOAD": "true",
        "SENTRY_AUTH_TOKEN": "eas_secret:SENTRY_AUTH_TOKEN",
        "SENTRY_ORG": "eas_secret:SENTRY_ORG",
        "SENTRY_PROJECT": "eas_secret:SENTRY_PROJECT",
        "EXPO_PUBLIC_SENTRY_DSN": "eas_secret:EXPO_PUBLIC_SENTRY_DSN",
        "EXPO_PUBLIC_SENTRY_ENABLED": "true"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "SENTRY_AUTH_TOKEN": "eas_secret:SENTRY_AUTH_TOKEN",
        "SENTRY_ORG": "eas_secret:SENTRY_ORG",
        "SENTRY_PROJECT": "eas_secret:SENTRY_PROJECT",
        "EXPO_PUBLIC_SENTRY_DSN": "eas_secret:EXPO_PUBLIC_SENTRY_DSN",
        "EXPO_PUBLIC_SENTRY_ENABLED": "true"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "ascAppId": "6753635113"
      }
    },
    "production": {
      "ios": {
        "ascAppId": "6753635113"
      }
    }
  }
}
